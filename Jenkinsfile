pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                script{
                    echo "Compiling the code"
                }
              
            }
        }
        stage('UnitTest') {
            steps {
                script{
                    echo "Running the test cases"
                }
              
            }
        }
        stage('Package') {
            steps {
                script{
                    echo "Packaging the code"
                }
              
            }
        }
    }
}
